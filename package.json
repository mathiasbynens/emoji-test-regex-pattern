{
  "name": "emoji-test-regex-pattern",
  "version": "2.0.2",
  "repository": "mathiasbynens/emoji-test-regex-pattern",
  "bugs": "https://github.com/mathiasbynens/emoji-test-regex-pattern/issues",
  "keywords": [
    "unicode",
    "regex",
    "regexp",
    "regular expressions",
    "code points",
    "symbols",
    "characters",
    "emoji"
  ],
  "license": "MIT",
  "author": {
    "name": "Mathias Bynens",
    "url": "https://mathiasbynens.be/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "if [ ! -f build-regexp ]; then curl --silent --location --output build-regexp https://github.com/s9e/RegexpBuilderCommand/releases/download/0.5.0/build-regexp.phar; chmod +x build-regexp; fi; ./build-regexp --version",
    "build": "node script/build.js; for v in dist/*; do echo; wc --bytes $v/*.txt; done",
    "test": "node test/test.js"
  },
  "devDependencies": {
    "regenerate": "^1.4.2",
    "unicode-emoji-13.0": "npm:@unicode/unicode-13.0.0@1.0.5",
    "unicode-emoji-13.1": "npm:@unicode/unicode-13.0.0@^1.2.1",
    "unicode-emoji-14.0": "npm:@unicode/unicode-14.0.0@^1.2.1",
    "unicode-emoji-15.0": "npm:@unicode/unicode-15.0.0@^1.2.2"
  }
}
